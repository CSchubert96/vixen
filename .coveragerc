[run]
branch = True
source = vixen
omit = */tests/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Re-enable standard pragma.
    pragma: no cover

    # Don't complain if tests don't hit defensive
    # assertion code:
    raise AssertionError
    raise NotImplementedError

    if __name__ == .__main__.:

ignore_errors = True
